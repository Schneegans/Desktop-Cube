#: resources/ui/settings.ui:63
msgid "General Options"
msgstr "Všeobecné možnosti"

#: resources/ui/settings.ui:90
msgid "Gap between last and first workspace"
msgstr "Medzera medzi poslednou a prvou pracovnou plochou"

#: resources/ui/settings.ui:99
msgid "You cannot switch from the last to the first workspace anyways."
msgstr "Nemôžete prepnúť z poslednej na prvú pracovnú plochu."

#: resources/ui/settings.ui:121 resources/ui/settings.ui:173
#: resources/ui/settings.ui:226 resources/ui/settings.ui:270
#: resources/ui/settings.ui:324 resources/ui/settings.ui:385
#: resources/ui/settings.ui:447 resources/ui/settings.ui:508
